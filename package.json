{
  "name": "tesla-mcp",
  "version": "0.1.0",
  "description": "MCP server for Tesla Fleet API â€” list cars, get location, wake up, and more",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sara3/Tesla-MCP.git"
  },
  "homepage": "https://github.com/Sara3/Tesla-MCP#readme",
  "keywords": [
    "tesla",
    "mcp",
    "model-context-protocol",
    "fleet-api",
    "vehicle",
    "location"
  ],
  "license": "MIT",
  "bin": {
    "tesla-mcp-server": "./build/index.js",
    "tesla-mcp-http": "./build/httpServer.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755'); require('fs').chmodSync('build/httpServer.js', '755');\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "get-token": "node --loader ts-node/esm src/getRefreshToken.ts",
    "test-api": "node --loader ts-node/esm src/testConnection.ts",
    "register": "node src/register.js",
    "start": "node run-mcp.js",
    "start:http": "node build/httpServer.js",
    "dev:http": "node --loader ts-node/esm src/httpServer.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "axios": "^1.8.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
